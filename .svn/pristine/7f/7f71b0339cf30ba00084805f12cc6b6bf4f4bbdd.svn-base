  margin: 1em 0;
}
p:first-child {
  margin-top: 0;
}
p:last-child {
  margin-bottom: 0;
}

h2 {
  font: 2.5em/1 amazon-ember, sans-serif;
  color: #232f3e;
}

h3 {
  font: 2.8em/1 amazon-ember, sans-serif;
  color: #232f3e;
}

ul,
ol {
