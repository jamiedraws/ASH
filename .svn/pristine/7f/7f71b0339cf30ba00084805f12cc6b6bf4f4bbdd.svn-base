.banner--ribbon::before, .banner--ribbon::after,
.banner--ribbon > span::before,
.banner--ribbon > span::after {
  content: "";
  position: absolute;
  width: 1em;
  border-style: solid;
}
.banner--ribbon::before,
.banner--ribbon > span::before {
