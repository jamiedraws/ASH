.toast__close {
  flex: 0 0 3em;
  width: 3em;
  height: 3em;
  display: block;
  border: none;
  background: none;
  position: relative;
}
.toast__close::before, .toast__close::after {
  content: "";
  width: 0.2em;
  height: inherit;
  background: currentColor;
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  top: 0;
  transition: transform 300ms ease-out 0.35s;
}
.toast__close::before {
  transform-origin: top right;
  transform: rotateZ(-45deg) scaleY(1.35);
  right: auto;
}
.toast__close::after {
  transform-origin: top left;
  transform: rotateZ(45deg) scaleY(1.35);
  transition-delay: 0.7s;
  left: auto;
}
.toast--hidden .toast__close::before {
  transform: rotateZ(-45deg) scaleY(0);
}
.toast--hidden .toast__close::after {
  transform: rotateZ(45deg) scaleY(0);
}

.toast {
  position: absolute;
  text-align: left;
  bottom: 0;
  left: 0;
  background: white;
  color: black;
  padding: 1em;
  box-shadow: 0 0 1.25em rgba(0, 0, 0, 0.35);
  transform: translateY(0);
  transition: transform 0.5s cubic-bezier(0.04, 0.64, 0.2, 1), opacity 0.3s ease-in-out;
}
.toast--hidden {
  transform: translateY(100%);
  opacity: 0;
}
.toast--hidden .toast__close::before {
  transition-delay: 0.35s;
}
.toast--hidden .toast__close::after {
  transition-delay: 0.7s;
}
.toast--alert {
  position: fixed;
  z-index: 5;
  display: flex;
  align-items: center;
  justify-content: space-between;
  transition-delay: 1s;
}
.toast__text {
  margin-right: 1em;
}
.toast__close {
  font-size: 0.45em;
}
.toast__close::before {
  transition-delay: 1.35s;
}
.toast__close::after {
  transition-delay: 1.7s;
}

